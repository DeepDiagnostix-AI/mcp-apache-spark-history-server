{
  "mcpServers": {
    "mcp-apache-spark-history-server": {
      "command": "/path/to/.local/bin/uv",
      "args": [
        "run",
        "--project",
        "/path/to/mcp-apache-spark-history-server",
        "python",
        "main_stdio.py"
      ],
      "env": {},
      "cwd": "/path/to/mcp-apache-spark-history-server",
      "timeout": 120000,
      "disabled": false
    }
  }
}
